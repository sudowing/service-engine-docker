{
  "name": "service-engine-docker",
  "version": "0.0.2",
  "description": "Dockerized Generic Service-Engine Service",
  "main": "lib/index.js",
  "scripts": {
    "start": "node lib/index",
    "lint": "npm run lint:tslint",
    "lint:fix": "npm run lint:prettier:fix",
    "lint:prettier": "npx prettier -l 'src/**/*.js'",
    "lint:prettier:fix": "npx prettier --write 'src/**/*.js'",
    "api-docs": "docker run -p 8080:8080 -e API_URL='http://0.0.0.0:8081/openapi' swaggerapi/swagger-ui",
    "make:api-md": "npx widdershins --search true --language_tabs 'javascript:JavaScript' 'javascript--nodejs:Node.JS' 'python:Python' 'go:Go' 'http:HTTP' 'ruby:Ruby' --summary http://0.0.0.0:8081/openapi -o docs/service.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sudowing/service-engine-docker.git"
  },
  "author": "open-source@joewingard.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sudowing/service-engine-docker/issues"
  },
  "homepage": "https://github.com/sudowing/service-engine-docker#readme",
  "dependencies": {
    "dotenv": "^8.2.0",
    "knex": "^0.21.1",
    "pg": "^8.2.1",
    "service-engine": "0.0.4",
    "widdershins": "^4.0.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.4"
  }
}
